class Solution {
public:
    int thirdMax(vector<int>& nums) {
        priority_queue<int> pq;
        unordered_set<int> st;
        for(auto x : nums){
            st.insert(x);
        }
        for(auto x : st){
            pq.push(x);
        }
        int count = 0;
        while(!pq.empty()){
            count++;
            if(count==3) return pq.top();
            pq.pop();
        }
        return *max_element(nums.begin(),nums.end());
    }
};
