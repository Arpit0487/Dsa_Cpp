class Solution {
public:
    vector<int> frequencySort(vector<int>& nums) {
        unordered_map<int,int> mp;
        for(auto x : nums){
            mp[x]++;
        }

        priority_queue<pair<int,int>, vector<pair<int,int>>, greater<>> pq;
        for(auto x : mp){
            pq.push({x.second, -x.first});
        }

        vector<int> ans;
        while (!pq.empty()) {
            auto [freq, num] = pq.top();
            pq.pop();
            int val = -num;
            for (int i = 0; i < freq; i++) {
                ans.push_back(val);
            }
        }

        return ans;        
    }
};
