class Solution {
public:
    int product(int x) {
        int prod = 1;
        while (x) {
            int y = x % 10;
            x /= 10;
            prod *= y;
        }
        return prod;
    }
    int smallestNumber(int n, int t) {
        while (true) {
            if (product(n) % t == 0)
                return n;
            n++;
        }
        return 0;
    }
};
