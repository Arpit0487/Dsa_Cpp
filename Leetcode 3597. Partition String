class Solution {
public:
    vector<string> partitionString(string s) {
        unordered_set<string> st;
        vector<string> ans;

        string ch ="";
        for(int i=0; i<s.size(); i++){
            ch += s[i];
            if(st.find(ch) == st.end()){
                //unique
                ans.push_back(ch);
                st.insert(ch);
                ch="";
            }
        }

        return ans;
    }
};
