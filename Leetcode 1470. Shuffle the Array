class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        vector<int> ans;
        int start = 0;
        int mid = n;
        while(mid < 2*n){
            ans.push_back(nums[start]);
            ans.push_back(nums[mid]);
            start++;
            mid++;
        }
        return ans;
    }
};
