class Solution {
public:
    int sumFourDivisors(vector<int>& nums) {
        int total = 0;
        for(int i=0; i<nums.size(); i++){
            int curr = 0;
            int num = 0;
            for(int j=1; j<=sqrt(nums[i]); j++){
                if(nums[i]%j==0){
                    num++;
                    curr+=j;
                    if(j!=nums[i]/j){
                        num++;
                        curr+=nums[i]/j;
                    }
                }
            }
            if(num==4) total+=curr;
        }
        return total;
    }
};
