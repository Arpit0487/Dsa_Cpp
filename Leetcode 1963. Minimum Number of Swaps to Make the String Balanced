class Solution {
public:
    int minSwaps(string s) {
        int point = s.length()-1;
        int swapp = 0;
        int Rcount = 0;
        int Lcount = 0;
        while(point>=0 && s[point]!='['){
            point--;
        }
        for(int i=0; i<s.length(); i++){
            if(s[i]=='[') Lcount++;
            else Rcount++;
            if(Rcount>Lcount){
                swap(s[i],s[point]);
                swapp++;
                point--;
                Lcount++;
                Rcount--;
                while(point>=0 && s[point]!='['){
                    point--;
                }
            }
        }
        return swapp;
    }
};
