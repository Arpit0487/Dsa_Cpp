class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int i = 0;
        while(i<bits.size()){
            if(bits[i]==0 && i==bits.size()-1) return true;
            else if((bits[i]==1 && bits[i+1]==0) || (bits[i]==1 && bits[i+1]==1)) i+=2;
            else if(bits[i]==0) i++;
        }
        return false;
    }
};
