class Solution {
public:
    vector<int> lastVisitedIntegers(vector<int>& nums) {
        vector<int> seen;
        vector<int> ans;
        int k = 0;
        int idx = 0;
        for (int i = 0; i < nums.size(); i++) {
            if (nums[i] == -1) {
                k++;
                if (k <= seen.size()) {
                    ans.push_back(seen[idx]);
                    idx++;
                }
                else{
                    ans.push_back(-1);
                }
            } else {
                k = 0;
                idx = 0;
                seen.insert(seen.begin(), nums[i]);
            }
        }
        return ans;
    }
};
