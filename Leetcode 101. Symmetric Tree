class Solution {
public:
    bool check(TreeNode* T1, TreeNode* T2) {
        if (T1 == NULL && T2 == NULL)
            return true;
        if (T1 == NULL || T2 == NULL)
            return false;
        if (T1->val != T2->val)
            return false;
        return check(T1->left, T2->right) && check(T1->right, T2->left);
    }

    bool isSymmetric(TreeNode* root) {
        if (root == NULL)
            return true;
        return check(root->left, root->right);
    }
};
