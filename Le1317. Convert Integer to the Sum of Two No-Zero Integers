class Solution {
public:
    bool nonzero(int x){
        while(x>0){
            if (x%10==0) return false;
            x/=10;
        }
        return true;
    }
    vector<int> getNoZeroIntegers(int n) {
        for(int i=1; i<n; i++){
            int a = i;
            int b = n-i;
            if(nonzero(a) && nonzero(b)){
                return {a,b};
            }
        }
        return {};
    }
};
