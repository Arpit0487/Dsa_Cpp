class Solution {
public:
    int minOperations(vector<int>& nums, vector<int>& target) {
        int count = 0;
        unordered_set<int> st;
        for(int i=0; i<nums.size(); i++){
            if(nums[i]==target[i]) continue;
            else if(st.find(nums[i])!=st.end()) continue;
            else{
                count++;
                st.insert(nums[i]);
            }
        }
        return count;
    }
};
