class Solution {
public:
    int getMaximumConsecutive(vector<int>& coins) {
        sort(coins.begin(), coins.end());
        int val = 0;
        int size = coins.size();
        for (int i = 0; i < size; i++) {
            if (coins[i] == val + 1 ||
                (coins[i] < val + 1 && (val + 1) - coins[i] <= val))
                val += coins[i];
            else
                break;
        }
        return val + 1;
    }
};
