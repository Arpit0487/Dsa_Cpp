class Solution {
public:
    vector<vector<int>> findWinners(vector<vector<int>>& matches) {
        unordered_map<int,int> loss;
        for(auto x : matches){
            loss[x[1]]++;
        }

        unordered_set<int> winners;
        vector<vector<int>> ans(2);
        for(auto x : matches){
            if(loss[x[0]] == 0){
                winners.insert(x[0]);
            }
        }

        ans[0] = vector<int>(winners.begin(), winners.end());
        
        for (auto &x : loss) {
            if (x.second == 1) ans[1].push_back(x.first);
        }

        sort(ans[0].begin(), ans[0].end());
        sort(ans[1].begin(), ans[1].end());

        return ans;
    }
};
