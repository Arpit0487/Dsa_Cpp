class Solution {
public:
    int findMinimumOperations(string s1, string s2, string s3) {

        if(s1[0]!=s2[0] || s2[0]!=s3[0] || s3[0]!=s1[0]) return -1;

        int x = s1.length();
        int y = s2.length();
        int z = s3.length();
        int n = min(x,min(y,z));

        int count =0;
        for(int i=0; i<n; i++){
            if(s1[i]!=s2[i] || s2[i]!=s3[i] || s3[i]!=s1[i]) break;
            count++;
        }
        int ans = 0;
        ans += x-count;
        ans += y-count;
        ans += z-count;

        return ans;
    }
};
